<?xml version='1.0' encoding='utf-8'?>
<odoo>
    <record id="gif_sale_order_inherit_view" model="ir.ui.view">
        <field name="name">sale.order.line.inherit.view.form</field>
        <field name="model">sale.order</field>
        <field name="inherit_id" ref="sale.view_order_form"></field>
        <field name="arch" type="xml">
          <xpath expr="//sheet//notebook//page[@name='order_lines']//field[@name='order_line']//tree//field[@name='qty_delivered']" position="after">            

            <field name="gif_real_stock1" />
            <field name="gif_total_units" />
            <field name='selected'/>


          </xpath>
          <xpath expr="//sheet//notebook//page[@name='order_lines']//field[@name='order_line']//tree//field[@name='product_template_id']" position="before">            

            <field name='selected'/>

          </xpath>
          <xpath expr="//field[@name='tipificacion_venta']" position="after">            

            <field name="Select_all"/>

          </xpath>
          
        </field>
    </record>

</odoo>